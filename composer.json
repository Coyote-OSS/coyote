{
    "name": "adam-boduch/coyote",
    "description": "4programmers.net",
    "version": "2.5",
    "homepage": "http://4programmers.net",
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Adam Boduch",
            "email": "adam@boduch.net"
        }
    ],
    "require": {
        "php": ">=7.3.0",
        "ext-gd": "*",
        "ext-json": "*",
        "adam-boduch/laravel-grid": "2.0.2",
        "barryvdh/laravel-debugbar": "^3.2",
        "doctrine/dbal": "^2.5",
        "elasticsearch/elasticsearch": "~6.7",
        "erusev/parsedown": "^1.7",
        "ezyang/htmlpurifier": "^4.7",
        "fideloper/proxy": "^4.0",
        "filp/whoops": "^2.3",
        "florianv/laravel-swap": "^1.1",
        "hedii/laravel-gelf-logger": "^4.1",
        "inacho/php-credit-card-validator": "^1.0",
        "intervention/image": "^2.4",
        "intervention/imagecache": "^2.3",
        "jenssegers/agent": "^2.3",
        "laravel/framework": "^6.0",
        "laravel/helpers": "^1.2",
        "laravel/passport": "^9",
        "laravel/socialite": "^5.0",
        "laravelcollective/html": "^6.0",
        "lavary/laravel-menu": "1.7.1",
        "league/flysystem-aws-s3-v3": "^1.0",
        "league/flysystem-cached-adapter": "^1.0",
        "mpdf/mpdf": "dev-development",
        "paylane/client": "^1.1",
        "predis/predis": "^1.0",
        "rawr/t-regx": "^0.9.8",
        "rcrowe/twigbridge": "^0.11",
        "sentry/sentry-laravel": "^1.2",
        "twig/extensions": "^1.5"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.5",
        "codeception/codeception": "^3.0",
        "mockery/mockery": "^1.0",
        "laravel/dusk": "^5.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database",
            "app/Models",
            "app/Plugins"
        ],
        "psr-4": {
            "Coyote\\": "app/"
        },
        "files": [
            "app/Helpers/Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled"
        ],
        "post-update-cmd": [
            "php artisan clear-compiled"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
